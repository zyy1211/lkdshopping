.item{
  width: 100%;
  padding: 10rpx 30rpx;
  font-size: 28rpx;
}
.weight{
  font-weight: bold;
}
.enrolless-box{
  font-size: 28rpx;
  margin-top: 20rpx;
}
.enrolless{
  width: 100%;
  padding: 15rpx 25rpx;
  margin-bottom: 2rpx;
}
.flex-end{
  width: 100%;
  display: flex;
  justify-content: flex-end;
}
.increase{
  width: 40rpx;
  height: 40rpx;
}
.increase image{
  width: 100%;
  height: 100%;
}
.wid-hei{
  width: 100%;
  height: 80rpx;
}
.name-text{
  width: 140rpx;
  height: 60rpx;
  font-weight: bold;
  line-height: 60rpx;
  text-align: right;
}
.date{
  width: 100%;
  font-size: 24rpx;
  height: 80rpx;
  color: #fe411b;
}
.input-name{
  width: 440rpx;
  height: 60rpx;
  padding: 0 10rpx;
  border: 1rpx solid rgba(229,229,229,1);
  position: relative;
}
.weight{
  font-weight: bold;
}
.cate-img{
  position: absolute;
  right: 15rpx;
}
.tick-png{
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
}
.input-selected{
  width: 120rpx;
  height: 60rpx;
  padding: 0 15rpx;
  border: 1rpx solid rgba(229,229,229,1);
  position: relative;
}
.draw{
  width: 25rpx;
  height: 15rpx;
}
.select-box{
  width: 100%;
  background-color: #eee;
  padding: 0 15rpx;
  position: absolute;
  top: 55rpx;
  left: 0;
  z-index: 999;
}
.select-option {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
}
.gender{
  width: 70rpx;
  height: 100%;
  margin-right: 20rpx;
}
.gender-box{
  width: 30rpx;
  height: 30rpx;
  border: 1rpx solid rgba(229,229,229,1);
}
.gender-box image{
  width: 30rpx;
  height: 30rpx;
}
.isActive{
  border: 0;
}
.signUp-box{
  width: 100%;
  height: 200rpx;
}
.signUp{
  width: 300rpx;
  height: 65rpx;
  margin: 60rpx auto;
  font-size: 28rpx;
  border-radius: 5rpx;
  background: linear-gradient(90deg,rgba(56,219,248,1),rgba(81,171,255,1));
}