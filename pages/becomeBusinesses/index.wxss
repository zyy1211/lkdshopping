.coach{
  width: 100%;
  padding: 30rpx 25rpx;
}
.coach-avatar{
  width: 100%;
}
.avatar{
  width: 150rpx;
  height: 150rpx;
  margin: 0 auto;
  border-radius: 5rpx;
}
.avatar image{
  width: 100%;
  height: 100%;
}
.click-modify{
  width: 100%;
  height: 80rpx;
  font-size: 28rpx;
}
.businesses{
  width:750rpx;
  height: 130rpx;
  font-size: 30rpx;
  padding:0rpx 25rpx;
}
.input{
  font-size: 26rpx;
}
.relative{
  position: relative;
}
.btn::after{
  border: none;
}
.btn{
  border: none;
  position: absolute;
  right: 0;
  bottom: 0;
  background:linear-gradient(90deg,rgba(56,219,248,1),rgba(81,171,255,1));
}
.placeholder{
 font-size: 26rpx;
 color:#A5A1A1;
}
.submit{
  width:360rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-top:100rpx;
  border-radius: 40rpx;
  background:rgba(56,219,248,1);
  font-family:MicrosoftYaHeiLight;
  font-weight:300;
  font-size:28rpx;
}
.submit::after{
  border:none;
}
.img{
  width:19rpx;
  height: 22rpx;
  margin-right: 5rpx;
}
.others{
  height: 100rpx;
  font-size:26rpx;
  margin-top:130rpx;
  font-family:MicrosoftYaHeiLight;
  color:#A5A1A1;
}
.business-picture{
  width: 100%;
  padding: 30rpx 25rpx;
}
.business-img{
  width: 500rpx;
  height: 300rpx;
  font-size: 28rpx;
  border: 1rpx solid rgba(245,245,245,1);
}
.business-img image{
  width: 100%;
  height: 100%;
}
.examine{
  width: 100%;
  font-size: 32rpx;
  padding: 10rpx 25rpx;
}
.examine-result{
  width: 100%;
  height: 70rpx;
}
.font{
  font-size: 24rpx;
  margin-top: 40rpx;
}
.note{
  width: 80%;
  margin: 0 auto;
  margin-top: 20rpx;
}
.weight{
  font-weight: bold;
}