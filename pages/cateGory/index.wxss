.cate{
  display: flex;
  width: 750rpx;
  height: 100%;
  position: fixed;
  top: 0;
}
.pro-nav{
  width:220rpx;
  height: 100%;
  background: rgba(239,244,247,1);
  overflow: scroll;
}
.nav-title{
  width:100%;
  height: 90rpx;
  line-height: 90rpx;
  font-size:28rpx;
}
/* 暂无数据 */
.nodata{
  width:580rpx;
  height: 100%;
  position: fixed;
  left:170rpx;
  bottom: 50rpx;
}
.proLists{
  width:100%;
  font-size:30rpx;
  padding:0 15rpx;
  margin-bottom: 85rpx;
}
.pro-title{
  width:100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size:32rpx;
  font-weight: 600;
}
.pro-lists{
  width:100%;
  display: flex;
  flex-wrap: wrap;
  overflow: scroll;
}
.pro-list{
  width:33%;
  height: 250rpx;
  margin: 10rpx 0;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.listImg{
  width:160rpx;
  height: 160rpx;
}
.pro-name{
  width:100%;
  font-size: 26rpx;
}

/* 模态框 */
.mask{
  top: 0;
}