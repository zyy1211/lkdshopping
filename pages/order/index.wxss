.con{
  margin: 0;
}
.navTab{
  width:100%;
  height: 80rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
}
.nav-title{
  width: 120rpx;
  height: 40rpx;
}
.isActive{
  border-bottom: 1rpx solid #3CBDF9;
  color:#3CBDF9;
}
.proLists{
  width:750rpx;
  margin-top: 100rpx;
}
.proInfo{
  width:100%;
  padding:0 40rpx;
  margin-top: 20rpx;
}
.pro-title{
  height: 80rpx;
  font-size: 28rpx;
}
.pro-order{
  height: 60rpx;
  font-size: 26rpx;
}
.pro-state{
  color: #3CBDF9;
}
.pro-img-count{
  width:100%;
  height: 200rpx;
  padding: 20rpx 0;
  font-size: 28rpx;
}
.pro-img{
  width: 190rpx;
  height: 153rpx;
  margin-right: 30rpx;
}
.pro-imgBox{
  width: 80%;
  height: 153rpx;
  flex-wrap: nowrap;
}
.finally{
  height: 120rpx;
  font-size: 28rpx;
}
.totalPri{
  color:rgba(219,33,33,1);
}
.isPay{
  width: 230rpx;
}
.logistics-info{
  width: 300rpx;
}
.border-width{
  width: 105rpx;
  height: 55rpx;
  border-radius: 13rpx;
}
.cancle{
  border: 1rpx solid rgba(152,152,152,1);
}
.confirm{
  border: 1rpx solid rgba(219,33,33,1);
  color: rgba(219,33,33,1);
}
.confirm-receive{
  width: 145rpx;
  height: 55rpx;
  border-radius: 13rpx;
  border: 1rpx solid rgba(152,152,152,1);
}

/* 暂无数据 */
.nodata{
  width:100%;
  height: 100%;
  position: fixed;
  left:0;
  top: 80rpx;
}