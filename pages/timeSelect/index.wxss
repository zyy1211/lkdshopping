.time{
  width: 100%;
  height: 90rpx;
  z-index: 99;
}
.time-1{
  width: 14%;
  font-size: 25rpx;
}
.time-1 text{
  display: block;
}
.active{
  color: #0099ff;
  border-bottom: 2rpx solid #0099ff;
}
.content{
  background-color: rgb(225, 225, 225,0.02);
  margin-top: 90rpx;
  margin-bottom: 250rpx;
  display: flex;
}
.left-ul{
  width: 160rpx;
  margin:50rpx 0 0 10rpx;
}
.left-li{
  font-size: 26rpx;
  height: 70rpx;
  margin: 10rpx 0;
}
.session{
  width: 85%;
  height: 100%;
  margin-right: 20rpx;
}
.session-ul{
  display: flex;
}
.session-li{
  width: 120rpx;
  margin:28rpx 0 0 10rpx;
  text-align: center;
}
.session-li text{
  font-size: 24rpx;
}
.session-li-1{
  font-size: 28rpx;
  width: 120rpx;
  height: 70rpx;
  margin: 10rpx 0rpx;
  border-radius: 8rpx;
}
.active1{
  background-color: #0099ff;
  color: #fff;
}
.isCheck{
  background: rgba(0, 0, 0,0.2);
  pointer-events: none;
}
.isBlank{
  background: rgba(0, 0, 0,0.2);
  pointer-events: none;
}
.isImg{
  width: 35rpx;
  height: 35rpx;
}

.selectL{
  width: 750rpx;
  height: 280rpx;
  z-index: 99;
}
.select-ul{
  width: 750rpx;
  height: 160rpx;
  padding: 20rpx 0;
  white-space: nowrap;
}
.yd,.select-li{
  margin: 0 20rpx;
  display: inline-block;
  font-size: 26rpx;
}
.yd{
  width:140rpx;
  height: 80%;
}
.select-li{
  width:160rpx;
  height: 90rpx;
  margin-top:10rpx;
  border:1rpx solid #0099ff;
  color:#0099ff;
  border-radius: 7rpx;
}
.rect{
  width:80%;
  height: 60rpx;
  margin:10rpx auto;
  border-radius: 7rpx;
}
.isTrue{
  background: #0099ff;
  border: 1rpx solid rgba(0, 0, 0,0);
}
.unKnow{
  border: 1rpx solid rgba(0, 0, 0,0.2);
}
.saled{
  background: rgba(0, 0, 0,0.2);
  border: 1rpx solid rgba(0, 0, 0,0);
}
.Y{
  width: 100%;
  height: 50%;
}
.btn{
  width: 700rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 10rpx;
}
.btnActive{
  color:#fff;
  background:#0099ff;
}

/* 模态框 */
.mask{
  top: 0;
}