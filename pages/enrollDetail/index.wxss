.item{
  width: 100%;
  padding: 10rpx 30rpx;
  font-size: 28rpx;
}
.weight{
  font-weight: bold;
}
.enrolless-box{
  font-size: 28rpx;
  margin-top: 20rpx;
}
.enrolless{
  width: 100%;
  padding: 15rpx 25rpx;
  margin-bottom: 5rpx;
}
.wid-hei{
  width: 100%;
  height: 66rpx;
}
.name-text{
  width: 140rpx;
  height: 66rpx;
  font-weight: bold;  
  line-height: 66rpx;
  text-align: right;
}
.input-name{
  width: 440rpx;
  height: 66rpx;
  padding: 0 10rpx;
  position: relative;
}
.weight{
  font-weight: bold;
}
.text-ellipsis{
  width: 92%;
  height: 60rpx;
  font-size: 28rpx;
  white-space: nowrap;
  text-overflow:ellipsis;
  position: absolute;
  left: 10rpx;
  top: 10rpx;
}
.cate-img{
  position: absolute;
  right: 15rpx;
}
.tick-png{
  width: 40rpx;
  height: 40rpx;
  margin-left: 10rpx;
}
.input-selected{
  width: 120rpx;
  height: 60rpx;
  padding: 0 15rpx;
  /* border: 1rpx solid rgba(229,229,229,1); */
  /* position: relative; */
}
.draw{
  width: 25rpx;
  height: 15rpx;
}
.gender{
  width: 70rpx;
  height: 100%;
  margin-right: 40rpx;
}
.gender-box{
  width: 30rpx;
  height: 30rpx;
  border: 1rpx solid rgba(229,229,229,1);
}
.gender-box image{
  width: 30rpx;
  height: 30rpx;
}
.isActive{
  border: 0;
}
.bill{
  width: 100%;
  height: 120rpx;
  margin-top: 20rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
}
.bill-info{
  height: 100%;
}
.bill-pay-box{
  width: 270rpx;
  height: 100%;
}
.bill-pay-btn{
  width: 150rpx;
  height: 65rpx;
  background: linear-gradient(90deg,rgba(56,219,248,1),rgba(81,171,255,1));
}
.date{
  width: 100%;
  padding: 0 30rpx 30rpx;
  font-size: 24rpx;
  color: #fe411b;
}
.signUp-box{
  width: 100%;
  height: 140rpx;
}
.signUp{
  width: 300rpx;
  height: 65rpx;
  margin: 60rpx auto;
  font-size: 28rpx;
  border-radius: 5rpx;
  background: linear-gradient(90deg,rgba(56,219,248,1),rgba(81,171,255,1));
}
.bg-gray{
  background: gray;
}